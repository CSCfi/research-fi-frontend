{
  "name": "research-fi",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n research-fi --i18n-format xlf --output-path i18n --i18n-locale fi && ng run research-fi:xliffmerge",
    "start-fi": "ng serve --configuration=fi",
    "start-sv": "ng serve --configuration=sv",
    "start-en": "ng serve --configuration=en",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:prod:fi": "ng build --configuration=production-fi",
    "build:prod:en": "ng build --configuration=production-en",
    "build:prod:sv": "ng build --configuration=production-sv",
    "build:server:prod:fi": "ng run research-fi:server:production-fi",
    "build:server:prod:en": "ng run research-fi:server:production-en",
    "build:server:prod:sv": "ng run research-fi:server:production-sv",
    "build:client-and-server-bundles": "npm run build:prod:fi && npm run build:prod:en && npm run build:server:prod:fi && npm run build:server:prod:en",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "compile_countup.js": "babel node_modules/countup.js -d node_modules/countup.js --presets @babel/preset-env",
    "postinstall": "npm run compile_countup.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@nguniversal/common": "^8.2.6",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "array-flat-polyfill": "^1.0.1",
    "bootstrap": "^4.4.1",
    "bowser": "2.5.2",
    "cacache": "^13.0.1",
    "compression": "^1.7.4",
    "core-js": "^2.6.11",
    "countup.js-angular2": "^7.3.0",
    "d3": "^5.15.0",
    "d3-zoom": "^1.8.3",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "helmet": "3.19.0",
    "jquery": "^3.4.1",
    "mississippi": "^4.0.0",
    "ngx-bootstrap": "^5.2.0",
    "popper.js": "^1.16.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "through2": "^3.0.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "^8.3.21",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.1",
    "https-proxy-agent": "^2.2.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.13.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.10"
  }
}
